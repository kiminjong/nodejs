<% include template/header %>
      <div class="container">
          <div class="jumbotron">
              <h1 class="display-3"><%= title %></h1>
              <p class="lead">Vao mongodb</p>
              <hr class="my-2">
          </div>
        <% data.forEach(function(motphantu){ %>
        <form action="/sua/<%= motphantu._id %>" method="post">
            <div class="form-group">
            <label for="ten">Ten</label>
            <input type="text"
                class="form-control" name="ten" id="" aria-describedby="helpId" placeholder="" value="<%= motphantu.ten %>">
            <small id="helpId" class="form-text text-muted">Help text</small>
            </div>
            <div class="form-group">
                <label for="tuoi">Tuoi</label>
                <input type="text"
                    class="form-control" name="tuoi" id="" aria-describedby="helpId" placeholder="" value="<%= motphantu.tuoi %>">
                <small id="helpId" class="form-text text-muted">Help text</small>
            </div> 
            <button type="submit" class="btn btn-dark">Sua</button>
        </form>
        <% }) %>
    </div>


<% include template/footer %>